@model Com.O2Bionics.ChatService.Web.Console.Models.TestO2BionicsMock.ChatSourceViewModel
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <title>O2Bionics Mock - Chat Source</title>
</head>
<body>
<div>
    <div class="widget-foot" style="width: 700px; margin-left: auto; margin-right: auto;">
        <div class="padd">
            @using (Html.BeginForm("ChatSource", "TestO2BionicsMock", new { ViewBag.ReturnUrl }, FormMethod.Post))
            {
                @Html.AntiForgeryToken()

                <div class="form-group">
                    @Html.LabelFor(m => m.FirstName, new { @class = "control-label col-lg-3" })
                    <div class="col-lg-9">
                        @Html.TextBoxFor(m => m.FirstName, new { @class = "form-control", placeholder = Html.DisplayNameFor(m => m.FirstName), })
                        @Html.ValidationMessageFor(m => m.FirstName, "", new { @class = "text-danger" })
                    </div>
                </div>
                <div class="form-group">
                    @Html.LabelFor(m => m.LastName, new { @class = "control-label col-lg-3" })
                    <div class="col-lg-9">
                        @Html.TextBoxFor(m => m.LastName, new { @class = "form-control", placeholder = Html.DisplayNameFor(m => m.LastName), })
                        @Html.ValidationMessageFor(m => m.LastName, "", new { @class = "text-danger" })
                    </div>
                </div>
                <div class="form-group">
                    @Html.LabelFor(m => m.Email, new { @class = "control-label col-lg-3" })
                    <div class="col-lg-9">
                        @Html.TextBoxFor(m => m.Email, new { @class = "form-control", placeholder = Html.DisplayNameFor(m => m.Email), })
                        @Html.ValidationMessageFor(m => m.Email, "", new { @class = "text-danger" })
                    </div>
                </div>
                <div class="form-group">
                    @Html.LabelFor(m => m.O2BionicsUserId, new { @class = "control-label col-lg-3" })
                    <div class="col-lg-9">
                        @Html.TextBoxFor(m => m.O2BionicsUserId, new { placeholder = Html.DisplayNameFor(m => m.O2BionicsUserId), })
                        @Html.ValidationMessageFor(m => m.O2BionicsUserId, "", new { @class = "text-danger" })
                    </div>
                </div>
                <div class="form-group">
                    @Html.LabelFor(m => m.O2BionicsAccountNumber, new { @class = "control-label col-lg-3" })
                    <div class="col-lg-9">
                        @Html.TextBoxFor(m => m.O2BionicsAccountNumber, new { placeholder = Html.DisplayNameFor(m => m.O2BionicsAccountNumber), })
                        @Html.ValidationMessageFor(m => m.O2BionicsAccountNumber, "", new { @class = "text-danger" })
                    </div>
                </div>
                <div>&nbsp;</div>
                <div class="form-group">
                    <div class="col-lg-9 col-lg-offset-3">
                        <input type="submit" value="Go to O2Chat Customer registration" class="btn btn-default"/>
                    </div>
                </div>
            }
        </div>
    </div>
</div>

@Scripts.Render("~/bundles/account/script")

</body>
</html>